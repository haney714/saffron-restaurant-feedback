<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:paddingTop="0dip" android:layout_gravity="top" >
           
            <TableRow
                android:paddingLeft="20dp"
                android:paddingTop="5dip">

                <TextView
                    android:id="@+id/title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:layout_span="1"
                    android:text="qwdwkhkh hgq"
                    android:textColor="#000000"
                    android:textSize="20dip"
                    android:textStyle="bold" />
            </TableRow>

            <TableRow
                android:paddingLeft="20dp"
                android:paddingTop="5dip">

                <TextView
                    android:id="@+id/description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:gravity="left"
                    android:text="sdffjhggg"
                    android:textColor="#000000"
                    android:textSize="16sp" />
            </TableRow>

            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginTop="3dip"
                    android:layout_marginRight="20dip"
                    android:layout_weight="1"
                    android:text="Food Quality :"
                    android:textColor="#2ecc71"
                    android:textSize="16sp" />
                
                <TextView
                    android:id="@+id/fQuality"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginRight="20dip"
                    android:layout_marginTop="3dip"
                    android:layout_weight="0"
                    android:text="first"
                    android:textColor="#2ecc71"
                    android:textSize="16sp" />
            </TableRow>
            
            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginTop="3dip"
                    android:layout_marginRight="20dip"
                    android:layout_weight="1"
                    android:text="Varity In Food :"
                    android:textColor="#2ecc71"
                    android:textSize="16sp" />
                
                <TextView
                    android:id="@+id/varityFood"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginRight="20dip"
                    android:layout_marginTop="3dip"
                    android:layout_weight="0"
                    android:text="first"
                    android:textColor="#2ecc71"
                    android:textSize="16sp" />
            </TableRow>
            
            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginTop="3dip"
                    android:layout_marginRight="20dip"
                    android:layout_weight="1"
                    android:text="Service :"
                    android:textColor="#2ecc71"
                    android:textSize="16sp" />
                
                <TextView
                    android:id="@+id/fService"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginRight="20dip"
                    android:layout_marginTop="3dip"
                    android:layout_weight="0"
                    android:text="first"
                    android:textColor="#2ecc71"
                    android:textSize="16sp" />
            </TableRow>
            
            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginTop="3dip"
                    android:layout_marginRight="20dip"
                    android:layout_weight="1"
                    android:text="Money For Value :"
                    android:textColor="#2ecc71"
                    android:textSize="16sp" />
                
                <TextView
                    android:id="@+id/moneyForValue"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginRight="20dip"
                    android:layout_marginTop="3dip"
                    android:layout_weight="0"
                    android:text="first"
                    android:textColor="#2ecc71"
                    android:textSize="16sp" />
            </TableRow>
            
            <TableRow
                android:paddingLeft="20dip"
                android:layout_width="match_parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dip"
                    android:layout_weight="0"
                    android:text="FeedBack :"
                    android:textSize="16sp" />
                
                <TextView
                    android:id="@+id/feedbackTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dip"
                    android:text="first"
                    android:scrollHorizontally="true"
                    android:textSize="16sp" />
            </TableRow>
</TableLayout>
